if sub_alarm(5000,5370,heathrow) == 2
or sub_alarm(5000,5360,heathrow) == 2
or sub_alarm(5000,5260,heathrow) == 2
or sub_alarm(5000,5240,heathrow) == 2
or sub_alarm(5000,5220,heathrow) == 2
or sub_alarm(5000,5350,heathrow) == 2
or sub_alarm(5000,5340,heathrow) == 2
or sub_alarm(5000,5330,heathrow) == 2
or sub_alarm(5000,5320,heathrow) == 2
or sub_alarm(5000,5200,heathrow) == 2
or sub_alarm(5000,5180,heathrow) == 2
or sub_alarm(5000,5160,heathrow) == 2
or sub_alarm(5000,5310,heathrow) == 2
or sub_alarm(5000,5300,heathrow) == 2
or sub_alarm(5000,5290,heathrow) == 2
or sub_alarm(5000,5280,heathrow) == 2
or sub_alarm(5000,5140,heathrow) == 2
or sub_alarm(5000,5120,heathrow) == 2
or sub_alarm(5000,5100,heathrow) == 2 then
begin
  sub_32_alarm_status = 2
end
if sub_alarm(5000,5370,heathrow) == 3
or sub_alarm(5000,5360,heathrow) == 3
or sub_alarm(5000,5260,heathrow) == 3
or sub_alarm(5000,5240,heathrow) == 3
or sub_alarm(5000,5220,heathrow) == 3
or sub_alarm(5000,5350,heathrow) == 3
or sub_alarm(5000,5340,heathrow) == 3
or sub_alarm(5000,5330,heathrow) == 3
or sub_alarm(5000,5320,heathrow) == 3
or sub_alarm(5000,5200,heathrow) == 3
or sub_alarm(5000,5180,heathrow) == 3
or sub_alarm(5000,5160,heathrow) == 3
or sub_alarm(5000,5310,heathrow) == 3
or sub_alarm(5000,5300,heathrow) == 3
or sub_alarm(5000,5290,heathrow) == 3
or sub_alarm(5000,5280,heathrow) == 3
or sub_alarm(5000,5140,heathrow) == 3
or sub_alarm(5000,5120,heathrow) == 3
or sub_alarm(5000,5100,heathrow) == 3 then
begin
  sub_32_alarm_status = 3
end
else
begin
  sub_32_alarm_status = 0
end

end


