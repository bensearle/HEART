end


object main_object (bg,fg,w,h,tbg,tfg)
begin
	clear
	lw = 2

	! SS98-2 TO WI New -21 !
	x = 400 y = -50 add
	y -= 50 add
	x -= 199 add
	y += 370 add
	line(fg=blue) clear
	x = 300 y = -100 cable_ident("blue","maggot","5.7") clear


	! SS98-1 TO SS130-1 !
	x = 350 y = -50 add
	y -= 25 add
	x -= 75 add
	y += 140 add
	x = 400 add
	y +=10 add
	line(fg=blue) clear

	! SS82-0 TO SS138-7 !
	x = 355 y = -225 add
	y -= 30 add
	x -= 110 add
	y += 35 add
	line(fg=blue) clear



	! SS82-2 TO SS98-5 !
	x = 405 y = -225 add
	y -= 30 add
	x += 150 add
	y += 190 add
	x -= 105 add
	y += 16 add
	line(fg=blue) clear
	x = 500 y = -65 cable_ident("blue","maggo","5.7") clear
	
	! SS82-4 TO SS54-7 !
	x = 510 y = -225 add
	y -= 80 add
	x -= 865 add
	y += 80 add
	line(fg=blue) clear
	x = 50 y = -305 cable_ident("blue","maggot","6.3") clear
	
	! WI-New -7 TO 103-2 !
	x = -49 y = 225 add
	y -= 290 add
	x += 49 add
	y += 17 add
	line(fg=blue) clear
	x = -25 y = -65 cable_ident("blue","maggot","5.7") clear

	! 103-5 TO 141-13 !
	x = 100 y = -49 add
	y -= 50 add
	x -= 205 add
	y += 50 add
	line(fg=blue) clear
	
	! 54a-8 TO 141-2 !
	x = -225 y = -49 add
	y -= 50 add
	x -= 135 add
	y += 50 add
	line(fg=blue) clear
	
	! 54a-1 TO 54-5 !
	x = -524 y = -49 add
	y -= 190 add
	x+=57 add
	y+=20 add
	line(fg=blue) clear

	! SS54-11 TO SS117-1 AND SS55-5 !
	x = -305 y = -225 add
	y -= 45 add
	x += 240 add
	y += 45 add
	line(fg=yellow) clear
	x = -250 y =-270 cable_ident("blue","maggot","4.1") clear
	x =-132 y =-270 cable_ident("blue","maggot","4.1") clear


	y = -225 x = -190 add line
	
	
	! x = -195 y = -225 add !  ! *** Sub 117, Connect Line Blocked *** !
	y -= 45 add
	line(fg=yellow) clear
	
	x = -305 y = -245 add
	y -= 10 add line(fg=blue) clear
	y = -270 x = -305 add x += 10 add line(fg=blue) clear
	x = -225 add x += 10 add line(fg=blue) clear
	x = -195 add x += 10 add line(fg=blue) clear
	x = -165 add x += 10 add line(fg=blue) clear
	
	x = -90 add x += 10 add line(fg=blue) clear
	x =- 67 add x += 3 add line(fg=blue) clear

	! x = -195 y = -230 add y -= 10 add line(fg=blue) clear !  ! *** Sub 117, Connect Line Blocked *** !
	! x = -195 y = -260 add y -= 10 add line(fg=blue) clear !  ! *** Sub 117, Connect Line Blocked *** !
	x = -65 y = -230 add y -= 10 add line(fg=blue) clear
	x = -65 y = -260 add y -= 10 add line(fg=blue) clear

	x = 450 y = 100 sub_130
	X=0 Y=-25 sub_103
	X=-499 Y=-25 sub_54a
	x = -480 y = -200 sub_54
	! x = -230 y = -200 sub_117 !     ! ** Sub 117 not required until further request from ADEM, Vijay Kara, 21/12/2007 **!
	x = -90 y = -200 sub_55
	x = 450 y = -200 sub_82

	x = 400 y = -25 sub_98

	X=26 Y=239 hv_1_west_intake_new
	X=180 Y=-92 sub_138

	X=-200 Y=-25 sub_141

	x = -570 y = 310 browser(1061,heathrow,1140)
end

mimic = main_object w=1140,h=800,just=centrecentre,fg=white,bg=aquamarine3,resizable

