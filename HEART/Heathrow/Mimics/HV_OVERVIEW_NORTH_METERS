object main_object (bg,fg,w,h,tbg,tfg)
begin
scale = 250
x = 0 y = 0 meter_button_north_1
x = 0 y = 40 meter_button_north_2

end

mimic = main_object w=250,h=200,just=centrecentre,fg=white,bg=black,resizable
