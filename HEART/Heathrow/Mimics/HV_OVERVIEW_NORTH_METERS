load mimic_libutil
load north_ocb_1_meter
load north_ocb_2_meter
load north_ocb_4_meter
load north_ocb_6_meter
load north_ocb_11_meter
load north_ocb_13_meter
load north_ocb_14_meter
load north_ocb_16_meter
load north_ocb_19_meter
load north_ocb_20_meter
load north_ocb_21_meter
load overview_meter_alarm_test


object meter_button_north_1

var call_var = popup north_ocb_1_meter
var alarm_colour_1 = loopback colour

begin

if meter_test(1,heathrow) == 1 then
   begin
   alarm_colour_1 = flashred
   end

else if meter_test(1,heathrow) == 2 then
   begin
   alarm_colour_1 = red
   end

else 
   begin
   alarm_colour_1 = decoroff
   end


just = centrecentre
x = 0 y = 0 w = 18 h = 18 rect(fg=alarm_colour_1)
x = -9 y = 9 add x += 18 add y -= 18 add line(lw=1,fg=decorshadow) clear
x = 9 y = -9 add x -= 18 add y += 18 add line(lw=1,fg=decorlight) clear

x = -5 y = -5 command_button("M","",12,"",ch=8)

set title call_var = "North Intake Meter 1"

if anyb then
  begin
    request call_var
  end


end

