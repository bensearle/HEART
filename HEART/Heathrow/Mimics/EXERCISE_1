Object main_object(w,h,fg,bg)
Begin
	!Draw bow tie shape!
	x=50 y=50 add
	y+=50 add
	x+=100 y-=50 add
	y+=50 add
	fg=lightgrey
	fill
	fg=black
	lw=3
	polygon
	
	!Draw circle shape!
	x=80 y=55 h=40 w=40 sa=0 aa=360
	fg=lightgrey
	pie
	fg=black
	arc	
End

mimic = main_object w=400,h=300,bg=lightblue,resizable
