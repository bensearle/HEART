object north_intake_unit
begin

just = centrecentre
fg=red
x = -500 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = -450 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = -350 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = -250 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = 0 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = 100 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = 150 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = 250 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = 400 y = 0 add y -= 150 add line(fg=red,lw=2) clear
x = 500 y = 0 add y -= 150 add line(fg=red,lw=2) clear


x = 0 y = 0 north_intake
scale = 200
y = -150 x = -500 ammeter(1,heathrow,"0-400A","SS 127")
y = -150 x = -450 ammeter(2,heathrow,"0-400A","SPARE")
y = -150 x = -350 ammeter(14,heathrow,"0-400A","SS 75")
y = -150 x = -300 ammeter(3,heathrow,"0-400A","SS 3")
y = -150 x = -250 ammeter(4,heathrow,"0-400A","SS 47")
y = -150 x =    0 ammeter(6,heathrow,"0-400A","SS 87")
y = -150 x =  100 ammeter(8,heathrow,"0-400A","SS 37")
y = -150 x =  150 ammeter(9,heathrow,"0-400A","SS 134")
y = -150 x =  200 ammeter(11,heathrow,"0-400A","EI I/C")
y = -150 x =  250 ammeter(10,heathrow,"0-400A","SS 5")
y = -150 x =  400 ammeter(12,heathrow,"0-400A","M1/2")
!y = -150 x =  450 ammeter(13,heathrow,"0-400A","EI P3")!
y = -150 x =  500 ammeter(17,heathrow,"0-400A","EI I/C")

scale=256
x = 200 y = -275 text(ch=12,just=centrecentre,tfg=black,"10 MVA") clear
x = 450 y = 110 text(ch=12,just=centrecentre,tfg=black,"Caesar Tx") clear

x = -300      
y = 0 add              
y = -65 add   
line(lw=2) clear

x = 200
y = 0 add
y = -65 add
line (lw=2) clear

x = 50
y = 0 add
y = 25 add
x = 150 add
y = 350 add
line (lw=2) clear

x = -150
y = 0 add
y = 25 add
x = -250 add
y = 350 add
line (lw=2) clear

scale = 100 
x = 300 y = 80 transformer("LT 1","") 
x = 450 y = 80 transformer("LT","") 

scale=200
x = -250 y = 180 
transformer_meter(15,5,heathrow,"NORTH","33/11kV Tx 1","0-1200A")
x =  150 y = 180 
transformer_meter(16,7,heathrow,"NORTH","33/11kV Tx 2","0-1200A")

scale = 120
x = -300 y = -25 ocb(1,480,18,42,heathrow,5,"top")
x = -250 y = 70 ocb(1,520,19,43,heathrow,8,"btm")
x = 150  y = 70 ocb(1,160,4,28,heathrow,12,"btm")
x = 200  y = -25 ocb_mont(1,470,21,45,heathrow,15,"top")

scale = 130
x = -250 y = 355 tap_change(20000,20599,1600,550,HEATHRO2,"33/11KV","24MVA",1,red,black)
x = 150 y = 355 tap_change(20000,20655,1601,551,HEATHRO2,"33/11KV","24MVA",2,red,black)

x = -50 y = 40 add text(scale=260,ch=20,tfg=red,"NORTH INTAKE") clear


end


object main_object (bg,fg,w,h,tbg,tfg)
begin

x = - 460 y = 275 deselect_box (1,4,heathrow,"Control Status")
clear

x = 0 
y = -100 north_intake_unit

scale = 215
X=266 Y=90 status_box(1,700,3000,3700,heathrow)
scale=256
x = -570 y = 310 browser(1075,heathrow,1140)
end

mimic = main_object w=1140,h=800,just=centrecentre,fg=white,bg=aquamarine3,resizable
