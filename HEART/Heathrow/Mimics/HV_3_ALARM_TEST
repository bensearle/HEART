object hv_3_alarm_status = int
begin

if sub_alarm(3000,3560,heathrow) > 0 
or sub_alarm(3000,3520,heathrow) > 0
or sub_alarm(3000,3530,heathrow) > 0
or sub_alarm(3000,3540,heathrow) > 0
or sub_alarm(3000,3550,heathrow) > 0
then
