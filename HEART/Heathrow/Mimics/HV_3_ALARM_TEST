  hv_3_alarm_status = 0

!
if sub_barcis_alarm(21214,heathrow) > 0
or sub_barcis_alarm(21254,heathrow) > 0  
or sub_barcis_alarm(21194,heathrow) > 0  
or sub_barcis_alarm(21174,heathrow) > 0  
