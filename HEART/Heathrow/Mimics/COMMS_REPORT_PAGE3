	         text_file = "<tr><td class=text>OS145 SUB 262</td><td class=text>" + fld_1 + "</td><td class=text>" + fld_2 + "</td><td class=text>" + fld_3 + "</td><td class=text>" + fld_4 + "</td><td class=text>" + fld_5 + "</td><td class=text>" + fld_6 + "</td></tr>"
	          request text_file
	end

	! *****  OUTSTATION 137  - Substation 293    *****!

              fld_1 = return_boolean_stats (heathro5,b2511,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC A DUTY  ****!
	fld_2 = return_boolean_stats (heathro5,b2512,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC B DUTY  ****!
	fld_3 = return_boolean_stats (heathro5,b2501,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC A SCADA TO PLC W/D FAULT ****!
	fld_4 = return_boolean_stats (heathro5,b2502,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC B SCADA TO PLC W/D FAULT ****!
	fld_5 = return_boolean_stats (heathro5,b2511,report_period,ft_transitions, getting_data,realf = fixed0)          !****  PLC A DUTY  ****!
     	fld_6 = return_boolean_stats (heathro5,b2512,report_period,ft_transitions, getting_data,realf = fixed0)          !****  PLC B DUTY  ****!
	if (getting_data==no) then
	begin
	         text_file = "<tr><td class=text>OS137 SUB 293</td><td class=text>" + fld_1 + "</td><td class=text>" + fld_2 + "</td><td class=text>" + fld_3 + "</td><td class=text>" + fld_4 + "</td><td class=text>" + fld_5 + "</td><td class=text>" + fld_6 + "</td></tr>"
	          request text_file
	end

	! *****  OUTSTATION 141  - Substation 280    *****!

	fld_3 = return_boolean_stats (heathro5,b11200,report_period,percent_time_true, getting_data,realf = fixed1)
	if (getting_data==no) then
	begin
		text_file = "<tr><td class=text>OS141 T2A Substations</td><td class=text>N/A</td><td class=text>N/A</td><td class=text>" + fld_3 + "</td><td class=text>N/A</td><td class=text>N/A</td><td class=text>N/A</td></tr>"
		request text_file
	end

	! *****  OUTSTATION 127  - Substation 228    *****!

              fld_1 = return_boolean_stats (heathro5,b3510,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC A DUTY  ****!
	fld_2 = return_boolean_stats (heathro5,b3511,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC B DUTY  ****!
	fld_3 = return_boolean_stats (heathro5,b3500,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC A SCADA TO PLC W/D FAULT ****!
	fld_4 = return_boolean_stats (heathro5,b3501,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC B SCADA TO PLC W/D FAULT ****!
	fld_5 = return_boolean_stats (heathro5,b3510,report_period,ft_transitions, getting_data,realf = fixed0)          !****  PLC A DUTY  ****!
     	fld_6 = return_boolean_stats (heathro5,b3511,report_period,ft_transitions, getting_data,realf = fixed0)          !****  PLC B DUTY  ****!
	if (getting_data==no) then
	begin
	         text_file = "<tr><td class=text>OS127 SUB 228</td><td class=text>" + fld_1 + "</td><td class=text>" + fld_2 + "</td><td class=text>" + fld_3 + "</td><td class=text>" + fld_4 + "</td><td class=text>" + fld_5 + "</td><td class=text>" + fld_6 + "</td></tr>"
	          request text_file
	end

	! *****  OUTSTATION 146  - Substation 226    *****!

              fld_1 = return_boolean_stats (heathro5,b14310,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC A DUTY  ****!
	fld_2 = return_boolean_stats (heathro5,b14311,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC B DUTY  ****!
	fld_3 = return_boolean_stats (heathro5,b14300,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC A SCADA TO PLC W/D FAULT ****!
	fld_4 = return_boolean_stats (heathro5,b14301,report_period,percent_time_true, getting_data,realf = fixed1)  !****  PLC B SCADA TO PLC W/D FAULT ****!
	fld_5 = return_boolean_stats (heathro5,b14310,report_period,ft_transitions, getting_data,realf = fixed0)          !****  PLC A DUTY  ****!
     	fld_6 = return_boolean_stats (heathro5,b14311,report_period,ft_transitions, getting_data,realf = fixed0)          !****  PLC B DUTY  ****!
	if (getting_data==no) then
	begin
	         text_file = "<tr><td class=text>OS146 SUB 226</td><td class=text>" + fld_1 + "</td><td class=text>" + fld_2 + "</td><td class=text>" + fld_3 + "</td><td class=text>" + fld_4 + "</td><td class=text>" + fld_5 + "</td><td class=text>" + fld_6 + "</td></tr>"
	          request text_file
	end


	text_file= "</table></center>" 
	request text_file
	text_file= "</body></html>" 
	request text_file
	SET RESET text_file

	FG=BLACK
	REALF=FIXED3
	FONT=FONT_NORMAL
	commstat_report=getting_data
end
