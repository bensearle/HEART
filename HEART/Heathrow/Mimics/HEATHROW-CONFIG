      control_user_number = user_number, !ro!
      control_wrkstn_number = wrkstn, !ro!
      control_inhibit = yesno, !wo!
      control_cancel = yesno, !wo!
      wcl_flag = yesno, !ro!
      in_control = yesno, !ro!
      alarm_forecast_time = ext_time, !ro!
      forecast_alarm_window_before = period_before,
      forecast_alarm_window_after = period_after,
      expiry_action_code = string_5, !wo!
