      system_message = string_255,
      system_command_text = cmd_text_type, !ro!
      system_message_time = ext_time, !ro!
      mail_message = string_255,
      mail_command_text = cmd_text_type, !ro!
      mail_message_time = ext_time, !ro!
      ws_quiet_fallback_level = alarm_level, !ro!
      ws_inactivity_timeout = inactivity_timeout_type,
      current_inactivity_timeout = inactivity_timeout_type, !ro!
      interface = yesno, !ro!
      supplied_client_id = int, !ro!
      unaccepted_mastered_alarms = pos_int, !ro!
      other_mastered_alarms = pos_int, !ro!
      current_interested_aois = set (aoi_number),
      authorised_interested_unaccepted = pos_int, !ro!
      authorised_interested_other = pos_int, !ro!
      unauthorised_interested = pos_int, !ro!
