      db_scan_number = mapping_scan_number, !ro!
      park_timeout = park_timeout, !wo!
      alarm_inhibit_timer = pos_number, !wo!
      alarm_disable_timer = pos_number, !wo!
      alarm_inhibit_due_time = ext_time,
      alarm_disable_due_time = ext_time,
      park_due_time = ext_time,
      profile_period = profile_period,
      diverted = yesno, !ro!
      park_count = byte, !ro!
