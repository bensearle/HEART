      current_access_level = set (access_level), !ro!
      current_mastered_areas = set (area),
      unaccepted_alarms = pos_int, !ro!
      overall_alarms = pos_int, !ro!
      user_number = user_number,
      user_name = name_type, !ro!
      user_description = name_type, !ro!
      user_default_areas = set (area), !ro!
      user_authorised_areas = set (area), !ro!
      user_access_level = set (access_level), !ro!
      change_time = ext_time, !ro!
      wrkstn_group = group,
      type_of_wrkstn = type_of_wrkstn_type,
      wrkstn_is_me = yesno, !ro!
      login_password = password_type, !wo!
