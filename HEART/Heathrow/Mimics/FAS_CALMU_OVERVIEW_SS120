	X=-307 Y=60 text(ch=12,"PANEL 1 TRANSFORMER 3")
	X+=230 Y=60 text(ch=12,"PANEL 2 TRANSFORMER 1")
	X+=230 Y=60 text(ch=12,"PANEL 7 TRANSFORMER 2")

