!*************** SUB 111 ***************!
 
object sub_111
begin
	sub_symbol_111
	just=topleft
	lw=2
	x = -150 y = 25 add y -= 50 add line (fg=yellow) clear
	x = -100 y = 25 add y -= 50 add line (fg=yellow) clear
	x = -25  y = 36 add x -= 50 add line (lw=4,fg=yellow) clear
	x =125  y = 36 add x -= 50 add line (lw=4,fg=red) clear
	x = 150 y = 25 add y -= 50 add line (fg=red) clear
	x = 200 y = 25 add y -= 50 add line (fg=red) clear

	scale = 310
	x=-150 y=0  	hv_lib_breaker("eclipse",40000,40130,1350,5540,5574,HEATHROW,1,"TOP",10)
	x=-100 y=0 	hv_lib_breaker("eclipse",40000,40150,1351,5541,5574,HEATHROW,2,"TOP",20)
	x = -50  y = 36 	hv_lib_breaker("eclipse",40000,40170,1352,5542,5574,HEATHROW,3,"SECT",30)
	x = 100  y = 36 	hv_lib_breaker("eclipse",40000,40290,1358,5548,5574,HEATHROW,8,"SECT",40)
	x = 150  y = 0 	hv_lib_breaker("eclipse",40000,40330,1360,5550,5574,HEATHROW,9,"TOP",50)
	x = 200  y = 0 	hv_lib_breaker("eclipse",40000,40350,1361,5551,5574,HEATHROW,10,"TOP",60)

end

